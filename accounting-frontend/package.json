  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:css && npm run generate && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/tailwind.css -o src/main.css",
